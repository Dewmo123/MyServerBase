<!--가독성을 위해 dataPacket을 제일 위부터 쓰기!!-->
<PDL>
	<!--DataPackets-->
	<dataPacket name ="VectorPacket">
		<float name="x"/>
		<float name="y"/>
		<float name="z"/>
	</dataPacket>
	
	<dataPacket name ="QuaternionPacket">
		<float name="x"/>
		<float name="y"/>
		<float name="z"/>
		<float name="w"/>
	</dataPacket>
	
	<!--Packets-->
	<packet name="Test">
		<list name="lists">
			<VectorPacket name="params"/>
		</list>
		<float name ="asd"/>
	</packet>
</PDL>